@model IEnumerable<miPrimerProyecto.Models.Reporte>

@{
    ViewBag.Title = "Reporte";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Reporte</h2>

<table class="table">
    <tr>
        @*<th>Cliente</th>*@
        <th>Nombre Cliente</th>
        <th>Documento Cliente</th>
        <th>Email Cliente</th>
        @*<th>Compra</th>
        <th>Fecha Compra</th>
        <th>Total Compra</th>*@
    </tr>
    @foreach (var item in Model)
     {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.nombreCliente)</td>
            <td>@Html.DisplayFor(modelItem => item.documentoCliente)</td>
            <td>@Html.DisplayFor(modelItem => item.emailCliente)</td>
            @*<td>@Html.DisplayFor(modelItem => item.fechaCompra)</td>
            <td>@Html.DisplayFor(modelItem => item.totalCompra)</td>*@
        </tr>
     }

</table>

@Html.ActionLink("Generar Reporte", "ImprimirReporte", "Cliente");
